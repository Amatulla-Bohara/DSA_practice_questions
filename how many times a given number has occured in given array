#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main(){
     int size_of_arr;
     cin>>size_of_arr;
     
     int number;
     cin>>number;
     
     int count=0;
     
     int arr[size_of_arr];
     
     for(int i=0; i<size_of_arr; i++){
     cin>>arr[i];
     if(arr[i]==number){
     count= count+1;
          }
     }
     cout<<"frequency of "<< number <<" is: " <<count;
     return 0;
}

/* input=> size_of_arr= 8
number = 1
arr[size_of_arr] = 1 2 3 4 1 1 2 1

output=> 4
